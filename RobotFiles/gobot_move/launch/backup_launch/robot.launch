  <node pkg="tf" type="static_transform_publisher" name="sonar2_to_base_link" args="0.1 -0.2 0 0 0 0 /base_link /sonar_two_range 50 "/>
  <node pkg="tf" type="static_transform_publisher" name="sonar3_to_base_link" args="-0.2 -0.1 0 0 3.14 0 /base_link /sonar_three_range 50 "/>
  <node pkg="tf" type="static_transform_publisher" name="sonar4_to_base_link" args="0.1 0.2 0 0 0 0 /base_link /sonar_four_range 50 "/>
-->
