<?xml version="1.0"?>
<launch>
  <arg name="model" default="$(find gobot_move)/urdf/hokuyo.urdf"/>
  <param name="robot_description" textfile="$(arg model)" />
  <param name="use_gui" value="False"/>

  <node pkg="hokuyo_node" type="hokuyo_node" name="hokuyo" >
    <param name="frame_id" value="base_laser" />
  </node>

  <node pkg="wheel" type="MD49.py" name="wheel" />
  <node pkg="sonar" type="sonar_distance.py" name="sonar" />
  <node pkg="gobot_move" type="sonar_range.py" name="sonar_range" />

  <node pkg="tf" type="static_transform_publisher" name="laser_to_base_link" args="0 0 0 0 0 0 /base_link /base_laser 50 "/>
<!--
